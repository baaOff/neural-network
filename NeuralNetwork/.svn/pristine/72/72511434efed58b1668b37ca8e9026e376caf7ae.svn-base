public class Element {
    public double health;
    public double knife;
    public double gun;
    public double enemy;
    double[] out = new double[NeuralNetwork.OUTPUT];

    public Element(double health,
                   double knife,
                   double gun,
                   double enemy,
                   double[] out){
        this.health = health;
        this.knife = knife;
        this.gun = gun;
        this.enemy = enemy;
        this.out = out;
    }
}
