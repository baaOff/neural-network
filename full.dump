import java.util.Random;

import model.Game;
import model.Move;
import model.Trooper;
import model.TrooperType;
import model.World;

public final class MyStrategy implements Strategy {
	private final Random random = new Random();
	
	@Override
	public void move(Trooper self, World world, Game game, Move move) {
		if (self.getActionPoints() > 0) {
			if(self.getType() == TrooperType.COMMANDER){
				ActionsCommander aCom = new ActionsCommander();
				aCom.work(self, world, game, move);
			}else if(self.getType() == TrooperType.FIELD_MEDIC){
				ActionsFieldMedik aFilMed = new ActionsFieldMedik();
				aFilMed.work(self, world, game, move);
			}else if(self.getType() == TrooperType.SOLDIER){
				ActionsSoldies aSold = new ActionsSoldies();
				aSold.work(self, world, game, move);
			}else if(self.getType() == TrooperType.SNIPER){
                 ActionsSniper aSniper = new ActionsSniper();
                 aSniper.work(self, world, game, move);
            }else {
                 ActionsScout aScout = new ActionsScout();
                 aScout.work(self, world, game, move);
            }
		}
	}
}


