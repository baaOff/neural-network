			move.setAction(ActionType.END_TURN);
			return;
		}
		getCommand(self, world);
		if (countInCommand() > 1) {
			int dist = (int) Math.round(getMaxLen(self, world, moving));
			// System.out.println(dist);
			if (countInCommand() == 2) {
				if (dist > 3) {
					actionUseGrenade(self, world, game, move);
					actionAttack(self, world, game, move);
					Trooper t = getMinFriend(self, world);
					Point po = moving.moveTo(self, world, moving.map,
							moving.ROW, moving.COL, self.getX(), self.getY(),
							t.getX(), t.getY());
					if (p.x == self.getX() && p.y == self.getY()) {
						move.setAction(ActionType.END_TURN);
						return;
					}
					moving.shift(self, po, world, game, move);
					System.out.println("ACTION SEARCH");
					return;
				}
			}
			if (dist > 4) {
				actionUseGrenade(self, world, game, move);
				actionAttack(self, world, game, move);
				Trooper t = getMinFriend(self, world);
				Point po = moving.moveTo(self, world, moving.map, moving.ROW,
						moving.COL, self.getX(), self.getY(), t.getX(),
						t.getY());
				if (p.x == self.getX() && p.y == self.getY()) {
					//move.setAction(ActionType.END_TURN);
					return;
				}
				moving.shift(self, po, world, game, move);
				System.out.println("ACTION SEARCH");
				return;
			}
		}
		moving.shift(self, p, world, game, move);
		System.out.println("ACTION SEARCH");
		actionUseGrenade(self, world, game, move);
		actionAttack(self, world, game, move);
	}
	
