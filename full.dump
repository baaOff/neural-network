import model.Game;
import model.Move;
import model.Trooper;
import model.TrooperType;
import model.World;

public class ActionsFieldMedik {

	private void actionMedHelp(Actions action, Trooper self, World world,
			Game game, Move move) {
		action.actionHide(self, world, game, move);
		action.getFriends(self, world);
		int tk = self.getActionPoints();
		if (self.getType() != TrooperType.FIELD_MEDIC)
			return;
		Trooper t = action.minInArrayList(action.friends, self, world);
		if (self.getDistanceTo(t) <=1){
			if (t.getHitpoints() < 100) {
				while (tk >= game.getFieldMedicHealCost()
						&& t.getHitpoints() < 100) {
					action.actionUseMedik(self, t, world, game, move);
					action.getFriends(self, world);
					tk -= game.getFieldMedicHealCost();
				}
			}
		}else{
			while(self.getDistanceTo(t)>1 && tk>0){
				action.randomMove(self, world, game, move, new Point(t.getX(), t.getY()));
				tk -= 2;
			}			
			if(t.getHitpoints() < 100){
				while(tk>=game.getFieldMedicHealCost() && t.getHitpoints()<100){
					action.actionUseMedik(self, t, world, game, move);
					action.getFriends(self, world);
					tk -= game.getFieldMedicHealCost();
				}
			}
		}
	}

	public void work(Trooper self, World world, Game game, Move move) {
		Moving moving = new Moving(self, world);
		Actions action = new Actions(self);
		actionMedHelp(action, self, world, game, move);
		action.actionUseMedBonus(self, world, game, move);
		if (self.getActionPoints() <= 6 && self.isHoldingFieldRation())
			action.actionUseEat(self, world, game, move);
		actionMedHelp(action, self, world, game, move);
		action.actionSearch(self, world, game, move);
		actionMedHelp(action, self, world, game, move);
	}

}


