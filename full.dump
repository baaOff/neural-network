	private void getAttack(Trooper self, World world, Game game, Move move) {
		if (self.getActionPoints() >= self.getShootCost()) {
			Trooper[] troopers = world.getTroopers();

			for (int i = 0; i < troopers.length; ++i) {
				Trooper trooper = troopers[i];
				boolean canShoot = world.isVisible(self.getShootingRange(),
						self.getX(), self.getY(), self.getStance(),
						trooper.getX(), trooper.getY(), trooper.getStance());
				if (canShoot && !trooper.isTeammate()) {
					// if (self.getActionPoints() < self.getShootCost())
					// break;
					move.setAction(ActionType.SHOOT);
					move.setX(trooper.getX());
					move.setY(trooper.getY());
					System.out.println("ACTION ATTACK");
					return;
				}
			} // end for
		}// end if

	}

	public void actionAttack(Trooper self, World world, Game game, Move move) {
		actionHide(self, world, game, move);
		getEnemies(self, world);
		countEnemy = enemies.size();
		int t = self.getActionPoints();
		while (t >= self.getShootCost() && countEnemy > 0) {
			getAttack(self, world, game, move);
			getEnemies(self, world);
			countEnemy = enemies.size();
			t -= self.getShootCost();
		}
	}

