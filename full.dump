	public void actionAttack(Trooper self, World world, Game game, Move move) {
		actionHide(self, world, game, move);
		getEnemies(self, world);
		countEnemy = enemies.size();
		int t = self.getActionPoints();
		while (t >= self.getShootCost() && countEnemy > 0) {
			getAttack(self, world, game, move);
			getEnemies(self, world);
			countEnemy = enemies.size();
			t -= self.getShootCost();
		}
	}

