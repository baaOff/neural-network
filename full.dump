	public void randomMove(Trooper self, World world, Game game, Move move,
			Point point) {
		if (self.getActionPoints() >= game.getStandingMoveCost()) { // Если
																	// достаточно
																	// очков
																	// действия
			CellType[][] cells = world.getCells(); // Получаем карту препятствий
													// на игровом поле

			int targetX = point.x; // Вычисляем координаты цели
									// X...
			int targetY = point.y; // ...и Y

			int offsetX = self.getX() > targetX ? -1 // Определяем смещение при
														// движении по оси X
					: self.getX() < targetX ? 1 : 0;

			int offsetY = self.getY() > targetY ? -1 // Определяем смещение при
														// движении по оси Y
					: self.getY() < targetY ? 1 : 0;

			boolean canMoveX = offsetX != 0 // Определяем необходимость и
											// возможность движения
					&& cells[self.getX() + offsetX][self.getY()] == CellType.FREE; // по
																					// оси
																					// X

			boolean canMoveY = offsetY != 0 // Определяем необходимость и
											// возможность движения
					&& cells[self.getX()][self.getY() + offsetY] == CellType.FREE; // по
																					// оси
																					// Y

			if (canMoveX || canMoveY) { // Если мы можем двигаться хотя бы в
										// одном направлении
				move.setAction(ActionType.MOVE); // Устанавливаем действие
													// "перемещение"

				if (canMoveX && canMoveY) { // Если можем двигаться в двух
											// направлениях
					if (rnd.nextInt() % 2 == 0) { // Выбираем направление
													// случайно
						move.setX(self.getX() + offsetX); // Устанавливаем
															// смещение по
															// горизонтали
						move.setY(self.getY()); // Оставляем текущую координату
												// Y
					} else {
						move.setX(self.getX()); // Оставляем текущую координату
												// X
						move.setY(self.getY() + offsetY); // Устанавливаем
															// смещение по
															// вертикали
					}
				} else if (canMoveX) { // Если можем двигаться только по
										// горизонтали
					move.setX(self.getX() + offsetX); // Устанавливаем смещение
														// по горизонтали
					move.setY(self.getY()); // Оставляем текущую координату Y
				} else { // Остался только один вариант: движение по вертикали
					move.setX(self.getX()); // Оставляем текущую координату X
					move.setY(self.getY() + offsetY); // Устанавливаем смещение
														// по вертикали
				}

				return; // Завершаем ход
			}
		}
